{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "publish:jsr": {
      "dependsOn": ["^publish:jsr"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
